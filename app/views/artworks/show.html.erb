<h2><%= @artwork.name %></h2>
<h3><%= @artwork.artist %></h3>
<h4>Museum: <%= link_to @artwork.museum.name, museum_path(@artwork.museum.id) %></h4>


<%= image_tag @artwork.image_url %>

<p><%= link_to "[Delete]", artwork_path(@artwork.id), method: :delete, data: { confirm: "Are you sure?" } %></p>
